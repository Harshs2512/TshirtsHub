{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        },
        {
            "src":"build/**",
            "use":"@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/signup",
            "dest": "backend/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}